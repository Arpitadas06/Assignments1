   "source": [
    "def fibonacci(n):\n",
    "    a, b = 0, 1\n",
    "    print(a, end=\" \")\n",
    "    for _ in range(1, n):\n",
    "        a, b = b, a + b\n",
    "        print(a, end=\" \")\n",
    "    print()\n",
    "\n",
    "num_terms = int(input(\"Enter the number of terms in the Fibonacci series: \"))\n",
    "fibonacci(num_terms)"
   ]
